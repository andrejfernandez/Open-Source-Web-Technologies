Cart] = useState([]);\n  const [total, setTotal] = useState(0);\n\n  return (\n    <div className=\"App\">\n      <Nav cart={cart} />\n      <Switch location={location} key={location.pathname}>\n        <Route path=\"/\" exact>\n          <Home />\n        </Route>\n        <Route path=\"/Menus\" exact>\n          <Menu\n            restaurants={restaurants}\n            setRestaurants={setRestaurants}\n            currentRestaurant={currentRestaurant}\n            setCurrentRestaurant={setCurrentRestaurant}\n            cart={cart}\n            setCart={setCart}\n            total={total}\n            setTotal={setTotal}\n          />\n        </Route>\n        <Route path=\"/CartView\" exact>\n          <CartView\n            cart={cart}\n            setCart={setCart}\n            total={total}\n            setTotal={setTotal}\n          />\n        </Route>\n        <Route path=\"/Checkout\" exact>\n          <Checkout />\n        </Route>\n      </Switch>\n      {/* <Footer /> */}\n    </div>\n  );\n}\n\nexport default App;\n","/home/andre/Repos/Open-Source-Web-Technologies/contactless-order-system/src/reportWebVitals.js",[],"/home/andre/Repos/Open-Source-Web-Technologies/contactless-order-system/src/pages/Home.js",[],"/home/andre/Repos/Open-Source-Web-Technologies/contactless-order-system/src/pages/Menus.js",["76","77","78","79","80"],"/home/andre/Repos/Open-Source-Web-Technologies/contactless-order-system/src/components/Nav.js",[],"/home/andre/Repos/Open-Source-Web-Technologies/contactless-order-system/src/components/Footer.js",[],"/home/andre/Repos/Open-Source-Web-Technologies/contactless-order-system/src/data/restaurantElements.js",["81"],"import { v4 as uuidv4 } from \"uuid\";\nimport temp from \"../images/temp.jpg\";\n\n// Restaurant 1\nimport r1logo from \"../images/Restaurant1/logo.jpg\";\nimport r1item1 from \"../images/Restaurant1/item1.png\";\nimport r1item2 from \"../images/Restaurant1/item2.png\";\nimport r1item3 from \"../images/Restaurant1/item3.p